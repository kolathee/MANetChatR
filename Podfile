target 'MANetChatR' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for MANetChatR

  target 'MANetChatRTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'MANetChatRUITests' do
    inherit! :search_paths
    # Pods for testing
  end

  pod 'Firebase'
  pod 'Firebase/Auth'
  pod 'Firebase/Database'
  pod 'Firebase/Storage'
